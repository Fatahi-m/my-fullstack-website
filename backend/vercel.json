{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/news",
        "dest": "/api/index.py"
      },
      {
        "src": "/api/news/(.*)",
        "dest": "/api/index.py"
      },
      {
        "src": "/api/businesses",
        "dest": "/api/index.py"
      },
      {
        "src": "/api/businesses/(.*)",
        "dest": "/api/index.py"
      }
    ]
  }