{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
    
      {
        "src": "/api/auth/(.*)",
        "methods": ["OPTIONS"],
        "dest": "/api/index.py"
      },
      
    
      {
        "src": "/api/(news|businesses)/(.*)",
        "methods": ["OPTIONS"],
        "dest": "/api/index.py"
      },
  
    
      {
        "src": "/api/(.*)",
        "methods": ["GET", "POST"],
        "dest": "/api/index.py"
      }
    ]
  }